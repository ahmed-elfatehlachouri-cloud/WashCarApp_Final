{
  "expo": {
    "name": "WashCar",
    "slug": "washcar-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-final.png",
    "scheme": "washcarapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.votreom.washcar"
    },
    "android": {
      "package": "com.votreom.washcar",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/logo-final.png"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo-final.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-final.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "87e84760-c2cd-4fdb-a9ce-89cc00b4dc07"
      }
    }
  }
}
